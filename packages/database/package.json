{
  "name": "database",
  "version": "0.0.0",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js"
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "prisma generate && tsc && mkdir -p dist/prisma/generated && cp -r prisma/generated/client dist/prisma/generated/client",
