    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "6.6.0"
  },
  "devDependencies": {
    "@types/node": "^24.0.12",
    "prisma": "6.6.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.5.3"
  }
}
