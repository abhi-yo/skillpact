{
  "version": 2,
  "builds": [
    {
      "src": "apps/api/serverless.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "apps/api/serverless.js"
    }
  ],
  "installCommand": "pnpm install",
  "buildCommand": "cd packages/database && pnpm build && cd ../../apps/api && pnpm build",
  "outputDirectory": "apps/api/dist"
}
